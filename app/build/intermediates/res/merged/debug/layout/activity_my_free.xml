<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F1F1F1"
    android:orientation="vertical">

    <com.lishi.baijiaxing.view.TopNavigationBar
        android:id="@+id/topbar_myfree"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="@color/base_bg_color"
        app:left_src="@drawable/back_top"
        app:text_title="我的免费领"
        app:title_color="@color/text_color"
        app:title_size="8sp"/>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/yiyuan_details_textColor"/>

    <android.support.v7.widget.RecyclerView
        android:id="@+id/recyclerView_myfree"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"></android.support.v7.widget.RecyclerView>
</LinearLayout>
